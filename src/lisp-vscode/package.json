{
  "name": "lisp",
  "publisher": "ixmilia",
  "version": "0.1.0",
  "description": "",
  "main": "out/extension.js",
  "engines": {
    "vscode": "^1.73.0"
  },
  "activationEvents": [
    "onLanguage:lisp",
    "onCommand:ixmilia-lisp.eval",
    "onCommand:ixmilia-lisp.restart"
  ],
  "extensionDependencies": [
    "ms-dotnettools.vscode-dotnet-runtime",
    "mattn.Lisp"
  ],
  "contributes": {
    "breakpoints": [
      {
        "language": "lisp"
      }
    ],
    "commands": [
      {
        "command": "ixmilia-lisp.eval",
        "title": "Evaluate LISP expression"
      },
      {
        "command": "ixmilia-lisp.restart",
        "title": "Restart LISP"
      }
    ],
    "debuggers": [
      {
        "type": "ixmilia-lisp",
        "label": "IxMilia Lisp",
        "languages": [
          "lisp"
        ],
        "configurationAttributes": {
          "launch": {
            "required": [
              "program"
            ],
            "properties": {
              "program": {
                "type": "string",
                "description": "The LISP file to debug."
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "ixmilia-lisp",
            "request": "launch",
            "name": "Launch LISP script",
            "program": "${workspaceFolder}/script.lisp"
          }
        ]
      }
    ],
    "keybindings": [
      {
        "command": "ixmilia-lisp.eval",
        "key": "alt+enter",
        "when": "editorLangId == lisp"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -watch -p ./",
    "clean-server": "npx rimraf ./server",
    "publish-server": "dotnet publish ../IxMilia.Lisp.EditorServer/IxMilia.Lisp.EditorServer.csproj --configuration Release",
    "copy-server": "npx copyfiles --up 8 --error --verbose \"../../artifacts/bin/IxMilia.Lisp.EditorServer/Release/net7.0/publish/*\" ./server/",
    "package": "npm run compile && npm run clean-server && npm run publish-server && npm run copy-server && npx vsce package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ixmilia/lisp.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ixmilia/lisp/issues"
  },
  "homepage": "https://github.com/ixmilia/lisp#readme",
  "devDependencies": {
    "@types/vscode": "1.73.0",
    "copyfiles": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.4",
    "vsce": "2.13.0"
  },
  "dependencies": {
    "@types/node": "18.11.9",
    "vscode-languageclient": "8.0.2"
  }
}